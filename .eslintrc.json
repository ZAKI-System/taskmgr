{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": ["warn", {"argsIgnorePattern": "^_"}],
        "no-empty": "warn",
        "semi": "error"
    }
}
